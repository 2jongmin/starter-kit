{
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "inlineSourceMap": false,
    "baseUrl": "src",
    "paths": {
      "components/*": ["./components/*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
